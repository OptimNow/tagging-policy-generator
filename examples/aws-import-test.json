{
  "tags": {
    "CostCenter": {
      "tag_key": {
        "@@assign": "CostCenter"
      },
      "enforced_for": {
        "@@assign": [
          "ec2:ALL_SUPPORTED",
          "lambda:ALL_SUPPORTED",
          "s3:ALL_SUPPORTED",
          "rds:ALL_SUPPORTED",
          "dynamodb:ALL_SUPPORTED"
        ]
      },
      "report_required_tag_for": {
        "@@assign": [
          "ec2:instance",
          "ec2:volume",
          "lambda:function",
          "s3:bucket",
          "rds:db",
          "dynamodb:table",
          "sagemaker:endpoint",
          "elasticloadbalancing:loadbalancer",
          "kinesis:stream"
        ]
      }
    },
    "Environment": {
      "tag_key": {
        "@@assign": "Environment"
      },
      "tag_value": {
        "@@assign": [
          "production",
          "staging",
          "development"
        ]
      },
      "enforced_for": {
        "@@assign": [
          "ec2:ALL_SUPPORTED",
          "rds:ALL_SUPPORTED"
        ]
      },
      "report_required_tag_for": {
        "@@assign": [
          "ec2:instance",
          "rds:db"
        ]
      }
    },
    "Owner": {
      "tag_key": {
        "@@assign": "Owner"
      },
      "enforced_for": {
        "@@assign": [
          "ec2:ALL_SUPPORTED",
          "s3:ALL_SUPPORTED",
          "lambda:ALL_SUPPORTED"
        ]
      },
      "report_required_tag_for": {
        "@@assign": [
          "ec2:instance",
          "s3:bucket",
          "lambda:function"
        ]
      }
    },
    "Project": {
      "tag_key": {
        "@@assign": "Project"
      },
      "tag_value": {
        "@@assign": [
          "alpha",
          "beta",
          "gamma"
        ]
      }
    },
    "Team": {
      "tag_key": {
        "@@assign": "Team"
      }
    }
  }
}
